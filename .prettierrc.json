{
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "proseWrap": "always",
  "endOfLine": "lf",
  "importOrder": [
    "^react",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^[~/]",
    "<TYPES>",
    "^[.]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
